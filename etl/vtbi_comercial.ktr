    <sql>SELECT vtiger_potential.potentialid,vtiger_potential.campaignid,vtiger_potential.related_to, vtiger_potential.amount,
       vtiger_products.productid,vtiger_products.vendor_id,vtiger_contpotentialrel.contactid,
       vtiger_quotes.quoteid,vtiger_salesorder.salesorderid,vtiger_invoice.invoiceid, potentialcrm.createdtime,pot_parent.setype
FROM vtiger_potential
INNER JOIN vtiger_crmentity as potentialcrm on potentialcrm.crmid=vtiger_potential.potentialid
LEFT JOIN vtiger_contpotentialrel on vtiger_potential.potentialid=vtiger_contpotentialrel.potentialid
LEFT JOIN (vtiger_seproductsrel,vtiger_products) on (vtiger_seproductsrel.crmid=vtiger_contpotentialrel.potentialid and vtiger_seproductsrel.productid=vtiger_products.productid)
LEFT JOIN vtiger_quotes on vtiger_potential.potentialid=vtiger_quotes.potentialid
LEFT JOIN vtiger_salesorder on vtiger_salesorder.quoteid=vtiger_quotes.quoteid
LEFT JOIN vtiger_invoice on vtiger_invoice.salesorderid=vtiger_salesorder.salesorderid
LEFT JOIN vtiger_crmentity as pot_parent on pot_parent.crmid=vtiger_potential.related_to
WHERE potentialcrm.deleted=0</sql>
