  <step>
    <name>vtbi_mktcontact</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>vtigercrm</connection>
    <sql>SELECT vtiger_campaigncontrel.*,vtiger_campaign.campaignstatus,vtiger_campaign.product_id,vtiger_potential.potentialid,
       vtiger_quotes.quoteid,vtiger_salesorder.salesorderid,vtiger_invoice.invoiceid,campaigncrm.createdtime
FROM vtiger_campaigncontrel
INNER JOIN vtiger_campaign on vtiger_campaign.campaignid=vtiger_campaigncontrel.campaignid
INNER JOIN vtiger_crmentity as campaigncrm on campaigncrm.crmid=vtiger_campaigncontrel.campaignid
INNER JOIN vtiger_crmentity as contactcrm on contactcrm.crmid=contactid
LEFT JOIN (vtiger_potential,vtiger_contpotentialrel) on (vtiger_potential.campaignid=vtiger_campaigncontrel.campaignid and vtiger_potential.potentialid=vtiger_contpotentialrel.potentialid and vtiger_contpotentialrel.contactid=vtiger_campaigncontrel.contactid)
LEFT JOIN vtiger_quotes on vtiger_potential.potentialid=vtiger_quotes.potentialid
LEFT JOIN vtiger_salesorder on vtiger_salesorder.quoteid=vtiger_quotes.quoteid
LEFT JOIN vtiger_invoice on vtiger_invoice.salesorderid=vtiger_salesorder.salesorderid
WHERE campaigncrm.deleted=0 and contactcrm.deleted=0</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
